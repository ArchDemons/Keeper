    <!-- +++++++++++++++++++++++++++++++++++++++ -->
    <!-- Campaign map, level selection -->
    <!-- +++++++++++++++++++++++++++++++++++++++ -->
    <screen id="selectCampaignLevel" controller="toniarts.openkeeper.game.state.MainMenuState">
        <layer id="layer" childLayout="vertical">
            <panel id="panel1" height="78%" width="100%" childLayout="vertical">
            </panel>
