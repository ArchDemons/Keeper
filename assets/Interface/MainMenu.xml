                    <control click="goToScreen(optionsMain)" id="back" name="cancelButton" width="48px" height="48px" margin="5px" />
                    <control click="applyGraphicsSettings()" id="start" name="okButton" width="48px" height="48px" margin="5px" />
                </panel>
