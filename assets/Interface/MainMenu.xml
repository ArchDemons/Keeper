                    <text id="singlePlayer" style="menuTitleText" text="${menu.28}" align="center" valign="bottom" />
                </panel>
            </panel>
            <panel id="panel2" height="38%" width="100%" align="center" valign="center" childLayout="vertical" visibleToMouse="true">
